    "url": "git+ssh://git@github.com/fhyxz1/codex-mcp-migrate.git"
  },
  "homepage": "https://github.com/fhyxz1/codex-mcp-migrate#readme",
  "bugs": {
    "url": "https://github.com/fhyxz1/codex-mcp-migrate/issues"
  },
  "license": "MIT",
  "dependencies": {
    "@iarna/toml": "^2.2.5",
    "commander": "^13.1.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^22.13.4",
    "typescript": "^5.7.3",
    "vitest": "^3.0.5"
