{
  "name": "codex-mcp-migrate",
  "version": "0.2.0",
  "description": "Flexible migration from mcpconfig.json/mcp-config.json to Codex config.toml",
  "type": "module",
  "bin": {
    "codex-mcp-migrate": "./dist/cli.js"
  },
  "files": [
